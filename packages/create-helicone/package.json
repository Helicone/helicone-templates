{
  "name": "create-helicone-app",
  "version": "0.1.0",
  "description": "CLI to generate Helicone-integrated projects from templates",
  "type": "module",
  "bin": {
    "create-helicone-app": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "helicone",
    "openai",
    "generator",
    "template",
    "cli"
  ],
  "author": "Helicone",
  "license": "MIT",
  "dependencies": {
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "degit": "^2.8.4",
    "execa": "^8.0.1",
    "ora": "^7.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/degit": "^2.8.6",
    "@types/node": "^20.10.5",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
